Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: white;
    line-width: 2;
    line-join: round;
  }
  [ OID_ =~'.*0$'] {
	  polygon-pattern-file: "0.jpg";
  }
  [ OID_ =~'.*1$'] {
	  polygon-pattern-file: "1.jpg";
  }
    [ OID_ =~'.*2$'] {
	  polygon-pattern-file: "2.jpg";
  }
    [ OID_ =~'.*3$'] {
	  polygon-pattern-file: "3.jpg";
  }
    [ OID_ =~'.*4$'] {
	  polygon-pattern-file: "4.jpg";
  }
    [ OID_ =~'.*5$'] {
	  polygon-pattern-file: "5.jpg";
  }
    [ OID_ =~'.*6$'] {
	  polygon-pattern-file: "6.jpg";
  }
    [ OID_ =~'.*7$'] {
	  polygon-pattern-file: "7.jpg";
  }
    [ OID_ =~'.*8$'] {
	  polygon-pattern-file: "8.jpg";
  }
    [ OID_ =~'.*9$'] {
	  polygon-pattern-file: "9.jpg";
  }
}